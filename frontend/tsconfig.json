{
  "compilerOptions": {
    "target": "es2017",
    "module": "ES2020",
    "moduleResolution": "node",
    "lib": ["es2017", "dom"],
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "sourceMap": true,
    "outDir": "dist",
    "typeRoots": ["node_modules/@types"]
  },
  "exclude": ["node_modules"]
}
